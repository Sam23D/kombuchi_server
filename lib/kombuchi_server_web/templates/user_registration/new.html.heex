<section class="w-full px-6 pb-12 antialiased bg-white">
  <div class="mx-auto max-w-7xl">

      <nav class="relative z-40 h-24 select-none" >
          <div class="container relative flex flex-wrap items-center justify-between h-24 mx-auto overflow-hidden font-medium border-b border-gray-200 md:overflow-visible lg:justify-center sm:px-4 md:px-2">
              <div class="flex items-center justify-start w-1/4 h-full pr-4">
                  <a href="#_" class="inline-block py-4 md:py-0">
                      <span class="p-1 text-xl font-black leading-none text-gray-900"><span>tails</span><span class="text-indigo-600">.</span></span>
                  </a>
              </div>
              <div class="top-0 left-0 items-start hidden w-full h-full p-4 text-sm bg-gray-900 bg-opacity-50 md:items-center md:w-3/4 md:absolute lg:text-base md:bg-transparent md:p-0 md:relative md:flex" :class="{'flex fixed': showMenu, 'hidden': !showMenu }">
                  <div class="flex-col w-full h-auto overflow-hidden bg-white rounded-lg md:bg-transparent md:overflow-visible md:rounded-none md:relative md:flex md:flex-row">
                      <a href="#_" class="inline-flex items-center block w-auto h-16 px-6 text-xl font-black leading-none text-gray-900 md:hidden">tails<span class="text-indigo-600">.</span></a>
                      <div class="flex flex-col items-start justify-center w-full space-x-6 text-center lg:space-x-8 md:w-2/3 md:mt-0 md:flex-row md:items-center">
                          <a href="/" class="inline-block w-full py-2 mx-0 ml-6 font-medium text-left text-indigo-600 md:ml-0 md:w-auto md:px-0 md:mx-2 lg:mx-3 md:text-center">Pagina inicio</a>
                      </div>
                      <div class="flex flex-col items-start justify-end w-full pt-4 md:items-center md:w-1/3 md:flex-row md:py-0">
                          <a href="#" class="w-full px-6 py-2 mr-0 text-gray-700 md:px-0 lg:pl-2 md:mr-4 lg:mr-5 md:w-auto">Inicia sesion</a>
                      
                      </div>
                  </div>
              </div>
          </div>
      </nav>
  </div>
</section>
<section class="bg-white py-20 lg:py-[80px] overflow-hidden relative z-10">
   <div class="container items-center max-w-6xl px-4 px-10 mx-auto sm:px-20 md:px-32 lg:px-16">
      <div class="flex flex-wrap lg:justify-between -mx-4">
         <div class="w-full lg:w-1/2 xl:w-6/12 px-4">
            <div class="max-w-[570px] mb-12 lg:mb-0">
               <h2 class=" text-dark mb-6 uppercase font-bold text-[32px] sm:text-[40px] lg:text-[36px] xl:text-[36px]">
                Registrate a Rica kambushi
               </h2>
            <div class="">
                <img class="h-32 sm:h-48 md:h-64 lg:h-80 w-full" src={ Routes.static_path(@conn, "/images/undraw_approve.svg") } />
            </div>


            </div>
         </div>
         <div class="w-full lg:w-1/2 xl:w-5/12 px-4">
            <div class="bg-white relative rounded-lg p-8 sm:p-12 shadow-lg">
                <.form let={f} for={@changeset} action="" >
                   
                    <div class="mb-6">
                        <label class="text-gray-600 text-lg" >Nombre</label> 
                        <%= text_input f, :name, placeholder: "Nombre", class: " w-full rounded py-3 px-[14px] text-body-color text-base border border-[f0f0f0] outline-none focus-visible:shadow-none focus:border-primary " %>
                      </div>
                      <div class="mb-6">
                        <label class="text-gray-600 text-lg" >Correo</label> 
                        <%= text_input f, :email, placeholder: "Correo electronico", class: " w-full rounded py-3 px-[14px] text-body-color text-base border border-[f0f0f0] outline-none focus-visible:shadow-none focus:border-primary " %>
                      </div>
                      <div class="mb-2">
                        <label class="text-gray-600 text-lg" >Password</label> 
                        <%= password_input  f, :password, required: true, placeholder: "Password", class: " w-full rounded py-3 px-[14px] text-body-color text-base border border-[f0f0f0] outline-none focus-visible:shadow-none focus:border-primary " %>
                      </div>
                      <div class="mb-6">
                        <%= password_input  f, :password_verify, required: true, placeholder: "Confirma tu password", class: " w-full rounded py-3 px-[14px] text-body-color text-base border border-[f0f0f0] outline-none focus-visible:shadow-none focus:border-primary " %>
                      </div>
                      <div>
                         <button type="submit" class="w-full px-3 py-4 font-medium text-white bg-blue-600 rounded-lg"
                         >Quiero registrarme</button>
                        </div>
                    <div>
                    </div>
        
                </.form>

            </div>
         </div>
      </div>
   </div>
</section>
<!-- ====== Contact Section End -->

